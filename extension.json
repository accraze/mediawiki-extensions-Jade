{
	"name": "JADE",
	"descriptionmsg": "jade-desc",
	"version": "0.0.1",
	"license-name": "GPL-3.0+",
	"type": "other",
	"author": [
		"Adam Roses Wight"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:JADE",
	"requires": {
		"MediaWiki": ">= 1.31.0"
	},
	"namespaces": [
		{
			"id": 810,
			"constant": "NS_JADE",
			"name": "Jade",
			"defaultcontentmodel": "JadeJudgment"
		},
		{
			"id": 811,
			"constant": "NS_JADE_TALK",
			"name": "Jade_talk"
		}
	],
	"AutoloadNamespaces": {
		"JADE\\": "includes/"
	},
	"ContentHandlers": {
		"JadeJudgment": "JADE\\ContentHandlers\\JudgmentContentHandler"
	},
	"MessagesDirs": {
		"JADE": [
			"i18n"
		]
	},
	"ServiceWiringFiles": [
		"includes/ServiceWiring.php"
	],
	"config": {
		"JadeAllowedScoringSchemas": {
			"value": {
				"diff": {
					"damaging": "damaging/v1.json",
					"goodfaith": "goodfaith/v1.json"
				},
				"revision": {
					"wp10": "wp10/v1.json"
				},
				"page": {
					"drafttopic": "drafttopic/v1.json"
				}
			}
		}
	},
	"manifest_version": 2
}
